{
  "pages": [
    "pages/main/index"
  ],
  "subpackages": [
    {
      "root": "pages/exception",
      "pages": [
        "error/index"
      ]
    },
    {
      "root": "pages/home",
      "pages": [
        "home/index"
      ]
    },
    {
      "root": "pages/order",
      "pages": [
        "sale-order-list/index",
        "purchase-order-list/index"
      ]
    },
    {
      "root": "pages/me",
      "pages": [
        "me/index",
        "user-info/index"
      ]
    },
    {
      "root": "pages/demo",
      "pages": [
        "index/index",
        "ui/index",
        "basic/index"
      ]
    }
  ],
  "preloadRule": {
    "pages/main/index": {
      "packages": [
        "pages/exception",
        "pages/home"
      ]
    },
    "pages/home/home/index": {
      "packages": [
        "pages/order",
        "pages/me"
      ]
    }
  },
  "window": {
    "navigationStyle": "custom"
  },
  "style": "v2",
  "renderer": "skyline",
  "rendererOptions": {
    "skyline": {
      "defaultDisplayBlock": true,
      "defaultContentBox": true,
      "disableABTest": true,
      "sdkVersionBegin": "3.0.0",
      "sdkVersionEnd": "15.255.255"
    }
  },
  "componentFramework": "glass-easel",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents"
}